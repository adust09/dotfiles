{
  "workbench.startupEditor": "none",
  "workbench.colorCustomizations": {
    "statusBar.background": "#2c2e2ea5",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#f06204"
  },
  "editor.formatOnSave": false,
  "editor.inlineSuggest.enabled": true,
  "window.autoDetectColorScheme": true,
  "window.newWindowDimensions": "inherit",
  "window.openFilesInNewWindow": "on",
  "deepl.apiKey": null,
  "terminal.integrated.fontFamily": "Inconsolata for Powerline",
  "redhat.telemetry.enabled": true,
  "vscode-pets.theme": "beach",
  "vscode-pets.petSize": "small",
  "cmake.configureOnOpen": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.watcherInclude": [
    "**/node_modules",
    "**/.git",
    "**/.DS_Store",
    "**/.vscode",
    "**/__pycache__"
  ],
  "terminal.integrated.scrollback": 2000000,
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": true,
  "editor.suggestSelection": "first",
  "breadcrumbs.enabled": true,
  "files.insertFinalNewline": true,
  "editor.fontFamily": "'Fira Code', Hasklig, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.renderLineHighlight": "all", 
  "editor.cursorBlinking": "smooth", 
  "editor.cursorSmoothCaretAnimation": "on", 
  "editor.cursorStyle": "block", 
  "files.autoGuessEncoding": true, 
  "[tex]": {
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.tabSize": 2
  },
  "[latex]": {
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.tabSize": 2
  },
  "[bibtex]": {
    "editor.tabSize": 2
  },
  "latex-workshop.intellisense.package.enabled": true,
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.ist",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.snm",
    "*.nav",
    "*.dvi",
    "*.synctex.gz"
  ],
  "latex-workshop.latex.outDir": "out",
  "latex-workshop.latex.recipes": [
    {
      "name": "latexmk",
      "tools": [
        "latexmk"
      ]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-silent",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ]
    }
  ],
  "solidity.compileUsingRemoteVersion": "v0.8.7+commit.e28d00a7",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "rust-analyzer.server.extraEnv": {
    "CARGO_NET_GIT_FETCH_WITH_CLI": "true"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[css]": {
    "editor.tabSize": 2
  },
  "npm.keybindingsChangedWarningShown": true,
  "git.autofetch": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.analysis.extraPaths": [],
  "python.autoComplete.extraPaths": [],
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false
  },
  "git.confirmSync": false,
  "git.ignoreRebaseWarning": true,
  "git.openRepositoryInParentFolders": "always",
  "cSpell.userWords": [
    "blocksremittances",
    "chrono",
    "dotenv",
    "Hasher",
    "hoge",
    "Insertable",
    "merkle",
    "plonky",
    "rootkinds",
    "Secp"
  ]
}
